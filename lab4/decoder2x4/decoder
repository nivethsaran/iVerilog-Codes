S_00000000008daa10 .scope module, "decoder2x4_tb" "decoder2x4_tb" 2 1;
 .timescale 0 0;
v00000000028425d0_0 .var "t_enable", 0 0;
v00000000028423f0_0 .var "t_xin", 1 0;
v0000000002842b70_0 .net "t_yout", 3 0, v0000000002842a30_0;  1 drivers
