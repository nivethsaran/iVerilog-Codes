S_00000000028053c0 .scope module, "forever_example" "forever_example" 2 1;
 .timescale 0 0;
v00000000011a7520_0 .var "clk", 0 0;
    .scope S_00000000028053c0;
T_0 ;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000011a7520_0, 0, 1;
T_0.0 ;
    %delay 5, 0;
    %load/vec4 v00000000011a7520_0;
    %nor/r;
    %store/vec4 v00000000011a7520_0, 0, 1;
    %jmp T_0.0;
    %end;
    .thread T_0;
    .scope S_00000000028053c0;
T_1 ;
    %vpi_call 2 13 "$monitor", "Time = %d  clk =%b", $time, v00000000011a7520_0 {0 0 0};
    %delay 100, 0;
    %vpi_call 2 14 "$finish" {0 0 0};
    %end;
    .thread T_1;
