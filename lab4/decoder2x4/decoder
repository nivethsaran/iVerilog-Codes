v0000000002842a30_0 .var "yout", 3 0;
E_00000000008dc220 .event edge, v00000000008da7b0_0, v0000000002842850_0;
    .scope S_00000000008dd2b0;
T_0 ;
    %wait E_00000000008dc220;
    %load/vec4 v00000000008da7b0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %load/vec4 v0000000002842850_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %jmp T_0.6;
T_0.2 ;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0000000002842a30_0, 0, 4;
    %jmp T_0.6;
T_0.3 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0000000002842a30_0, 0, 4;
    %jmp T_0.6;
T_0.4 ;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0000000002842a30_0, 0, 4;
    %jmp T_0.6;
T_0.5 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0000000002842a30_0, 0, 4;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0000000002842a30_0, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_00000000008daa10;
T_1 ;
    %vpi_call 2 8 "$monitor", "%b %b %b ", v00000000028423f0_0, v00000000028425d0_0, v0000000002842b70_0 {0 0 0};
    %vpi_call 2 9 "$display", "Xin Enable yout" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000000028425d0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v00000000028423f0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000028425d0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v00000000028423f0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000028425d0_0, 0, 1;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v00000000028423f0_0, 0, 2;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000000028425d0_0, 0, 1;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v00000000028423f0_0, 0, 2;
