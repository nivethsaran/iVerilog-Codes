    .port_info 0 /INPUT 2 "xin";
    .port_info 1 /INPUT 1 "enable";
    .port_info 2 /OUTPUT 4 "yout";
v00000000008da7b0_0 .net "enable", 0 0, v00000000028425d0_0;  1 drivers
